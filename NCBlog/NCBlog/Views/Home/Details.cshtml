@model BlogPostViewModel

<div class="col-md-12 col-12 paddding animate-box" data-animate-effect="fadeIn">
    <div class="fh5co_suceefh5co_height">
        <img src="~/images/2.jpg" alt="img" />
        <div class="fh5co_suceefh5co_height_position_absolute"></div>
        <div class="fh5co_suceefh5co_height_position_absolute_font">
            <div class="">
                <a href="#" class="color_fff">
                    <i class="fa fa-clock-o"></i>&nbsp;&nbsp;Dec 31,2017
                </a>
            </div>
            <div class=""><h2>@Model.Title</h2></div>
        </div>
    </div>
</div>

<div id="fh5co-title-box" style="background-image: url(images/camila-cordeiro-114636.jpg); background-position: 50% 90.5px;" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="page-title">
        <img src="~/images/2.jpg" alt="Free HTML5 by FreeHTMl5.co">
        <span>January 1, 2018</span>
        <h2>@Model.Title</h2>
    </div>
</div>
<div id="fh5co-single-content" class="container-fluid pb-4 pt-4 paddding">
    <div class="container paddding">
        <div class="row mx-0">
            <div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
                <p>@Html.Raw(Model.Description)</p>
            </div>

        </div>
    </div>
</div>


